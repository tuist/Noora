#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/Noora/Noora/node_modules/.pnpm/miniflare@4.20250508.3/node_modules/miniflare/node_modules:/home/runner/work/Noora/Noora/node_modules/.pnpm/miniflare@4.20250508.3/node_modules:/home/runner/work/Noora/Noora/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/Noora/Noora/node_modules/.pnpm/miniflare@4.20250508.3/node_modules/miniflare/node_modules:/home/runner/work/Noora/Noora/node_modules/.pnpm/miniflare@4.20250508.3/node_modules:/home/runner/work/Noora/Noora/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../miniflare/bootstrap.js" "$@"
else
  exec node  "$basedir/../miniflare/bootstrap.js" "$@"
fi
